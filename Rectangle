/**************************************
 ******PLOT ORBITAL WAVEFUNCTIONS******
 *************************************/
#include<stdio.h>
#include<math.h>
#include <time.h>
#include <sys/time.h>
int * gen(){
  struct timeval tv;
  gettimeofday(&tv, NULL);
  static int digits[1000];
  int value = tv.tv_usec;
  int old = value%10000;
  for (int i=1; i<=1000; i++)
  {
    digits[i] = value;
    printf("%d\t", value%100);
    if (value>=100)
    {
      value = value/100;
    }
    else
    {
      value = old*old/100;
      if (value%100==0){
        value=value/100;
      }
      else if(value%10000<100){
        value=value%10000+value/100;
      }
      else{
        value=value%10000;
      }
      if (value<100){
        value=value*value;
      }
      old = value;
    }
  }
  return digits;
}
int main(){
  int *digits = gen();
  FILE *fp1=NULL;
  fp1=fopen("orb1.txt","w");
  for(int i=1;i<=1000;i+=2){
    if (digits[i]>=10){
      digits[i]=digits[i]%100;
    }
    if (digits[i+1]>=10){
      digits[i+1]=digits[i+1]%100;
    }
    fprintf(fp1,"%d\t%d\n", digits[i],digits[i+1]);
  }
}
