/**************************************
 ******PLOT ORBITAL WAVEFUNCTIONS******
 *************************************/
#include<stdio.h>
#include<math.h>
#include <time.h>
#include <sys/time.h>
int * gen(){
  struct timeval tv;
  gettimeofday(&tv, NULL);
  static int digits[100];
  int value = tv.tv_usec;
  int old = value;
  int old2 = value;
  int t;
  int n;
  for (int i=1; i<=100; i++)
  {
    t = 9;
    n = 0;
    for (;;)
    {
      if (value%t==0)
      {
        if (value%10>=5)
        {
          digits[i] = 9-n;
          printf("%d", digits[i]);
        }
        else
        {
          digits[i] = 0+n;
          printf("%d", digits[i]);
        }
        break;
      }
      t-=2;
      n++;
    }
    if (value>=10)
    {
      value = value/10;
    }
    else
    {
      old = value;
      value = old2+tv.tv_usec;
      old2 = value;
      if (value==old)
      {
        value = (value*value)%1000000;
      }

    }
  }
  return digits;
}
int main(){
  int *digits = gen();
  FILE *fp1=NULL;
  fp1=fopen("orb2.txt","w");
  for(int i=1;i<=1000;i+=2){
    if (digits[i]>=10){
      digits[i]=digits[i]%10;
    }
    if (digits[i+1]>=10){
      digits[i+1]=digits[i+1]%10;
    }
    fprintf(fp1,"%d\t%d\n", digits[i],digits[i+1]);
  }
}
