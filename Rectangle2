/**************************************
 ******PLOT ORBITAL WAVEFUNCTIONS******
 *************************************/
#include<stdio.h>
#include<math.h>
#include <time.h>
#include <sys/time.h>
int * gen(){
  struct timeval tv;
  gettimeofday(&tv, NULL);
  static int digits[1000];
  int value = tv.tv_usec;
  int old = value;
  int old2 = value;
  int t;
  int n;
  for (int i=1; i<=1000; i++)
  {
    t = 99;
    n = 0;
    for (;;)
    {
      if (value%t==0)
      {
        if (value%10>=5)
        {
          digits[i] = 99-n;
          printf("%d\t", digits[i]);
        }
        else
        {
          digits[i] = 0+n;
          printf("%d\t", digits[i]);
        }
        break;
      }
      t-=2;
      n++;
    }
    if (value>=100)
    {
      value = value/100;
    }
    else
    {
      value = old2*old2/100;
      if (value%100==0){
        value=value/100;
      }
      else if(value%10000<100){
        value=value%10000+value/100;
      }
      else{
        value=value%10000;
      }
      // if (value==old)
      // {
      //   value = (value*value)%1000000;
      // }
      old2 = value;
    }
  }
  return digits;
}
int main(){
  int *digits = gen();
  FILE *fp2=NULL;
  fp2=fopen("orb2.txt","w");
  for(int i=1;i<=1000;i+=2){
    // if (digits[i]>=10){
    //   digits[i]=digits[i]%10;
    // }
    // if (digits[i+1]>=10){
    //   digits[i+1]=digits[i+1]%10;
    // }
    fprintf(fp2,"%d\t%d\n", digits[i],digits[i+1]);
  }
}
